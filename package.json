{
    "name": "@lesnoypudge/eslint-react-config",
    "version": "2.0.1",
    "description": "lesnoypudge's eslint-react-config",
    "main": "config.js",
    "type": "module",
    "scripts": {
        "lint:check": "npx eslint .",
        "lint:fix": "npx eslint . --fix",
        "formatter:config": "npx formatter-config",
        "ts:init": "tsc --init",
        "deploy": "git add . && git commit -m 'update' && git push && git status",
        "publish": "npm publish --access public"
    },
    "keywords": [
        "lesnoypudge",
        "eslint",
        "config",
        "base"
    ],
    "author": "LesnoyPudge",
    "license": "ISC",
    "dependencies": {
        "@eslint/compat": "^1.0.3",
        "eslint": "^9.4.0",
        "eslint-plugin-jsx-a11y": "^6.8.0",
        "eslint-plugin-jsx-control-statements": "^3.0.0",
        "eslint-plugin-react": "^7.34.2",
        "eslint-plugin-react-hooks": "^4.6.2",
        "eslint-plugin-react-refresh": "^0.4.7",
        "typescript-eslint": "^7.12.0"
    },
    "devDependencies": {
        "@lesnoypudge/eslint-base-config": "^1.7.1",
        "@lesnoypudge/formatter-config": "^1.1.0",
        "@lesnoypudge/types-global-react": "^2.0.1",
        "eslint-plugin-only-warn": "^1.1.0",
        "typescript": "^5.4.5"
    },
    "overrides": {
        "eslint": "^9.4.0",
        "typescript": "^5.4.5"
    }
}
